{
    "name": "cryental/volistx-skeleton",
    "description": "Reusable Framework for Volistx API Based Lumen PHP.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "fakerphp/faker": "^1.9.1",
        "fideloper/proxy": "^4.4",
        "geoip2/geoip2": "^2.11",
        "http-interop/http-factory-guzzle": "^1.0",
        "illuminate/redis": "^8.46",
        "intervention/image": "^2.5",
        "jdavidbakr/cloudfront-proxies": "^1.6",
        "laravel/lumen-framework": "^8.0",
        "laravel/scout": "^9.1",
        "laravelista/lumen-vendor-publish": "8.0",
        "league/flysystem": " ~1.0",
        "maxsky/lumen-app-key-generator": "^1.0",
        "meilisearch/meilisearch-php": "^0.18.2",
        "mlntn/lumen-artisan-serve": "^1.3",
        "paragonie/random-lib": "^2.0",
        "rennokki/laravel-eloquent-query-cache": "^2.6",
        "rogervila/lumen-rate-limiting": "^1.0",
        "spatie/laravel-responsecache": "^7.0",
        "staudenmeir/eloquent-has-many-deep": "^1.13",
        "swooletw/laravel-swoole": "^2.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.19"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Illuminate\\": "Illuminate/"
        },
        "files": [
            "app/Helpers.php"
        ],
        "classmap": [
            "app/Classes"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ]
    }
}
