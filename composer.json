{
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "chuckrincon/lumen-config-discover": "^1.0",
    "cryental/laravel-stackpath": "^1.0",
    "cryental/lumen-generator": "^8.2",
    "fakerphp/faker": "^1.19",
    "fideloper/proxy": "^4.4",
    "hhxsv5/laravel-s": "^3.7",
    "http-interop/http-factory-guzzle": "^1.2",
    "illuminate/redis": "^9.3",
    "intervention/image": "^2.7",
    "jikan-me/jikan": "^3.0",
    "laravel/lumen-framework": "^9.0",
    "league/flysystem": "^3.0",
    "maxsky/lumen-app-key-generator": "^1.0",
    "monicahq/laravel-cloudflare": "^3.2",
    "rennokki/laravel-eloquent-query-cache": "^3.3.0",
    "staudenmeir/eloquent-has-many-deep": "^1.15",
    "teamtnt/laravel-scout-tntsearch-driver": "^11.6"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers.php"
    ],
    "classmap": [
      "app/Classes"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ],
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ]
  }
}
