{
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "chuckrincon/lumen-config-discover": "^1.0",
    "cryental/larasupport": "^1.7",
    "cryental/laravel-stackpath": "^2.0",
    "fakerphp/faker": "^1.19",
    "filp/whoops": "^2.14",
    "flipbox/lumen-generator": "^9.1",
    "hhxsv5/laravel-s": "^3.7",
    "http-interop/http-factory-guzzle": "^1.2",
    "illuminate/redis": "^10.0",
    "intervention/image": "^2.7",
    "jikan-me/jikan": "^4.0",
    "laravel/lumen-framework": "^10.0",
    "league/flysystem": "^3.0",
    "monolog/monolog": "^3.3",
    "paragonie/random-lib": "^2.0",
    "rennokki/laravel-eloquent-query-cache": "^3.3.0",
    "rogervila/lumen-rate-limiting": "^1.1",
    "spatie/laravel-responsecache": "^7.4",
    "spatie/temporary-directory": "^2.1",
    "staudenmeir/eloquent-has-many-deep": "^1.15",
    "suin/php-rss-writer": "^1.6"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "phpunit/phpunit": "^9.3",
    "spatie/ray": "^1.37"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "classmap": [
      "app/Classes"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1",
      "ext-pcntl": "8.1",
      "ext-posix": "8.1"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ],
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ]
  }
}
