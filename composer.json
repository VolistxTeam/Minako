{
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "chuckrincon/lumen-config-discover": "^1.0",
    "cryental/larasupport": "^1.7",
    "cryental/laravel-stackpath": "^1.0",
    "fakerphp/faker": "^1.19",
    "fideloper/proxy": "^4.4",
    "filp/whoops": "^2.14",
    "flipbox/lumen-generator": "^9.1",
    "hhxsv5/laravel-s": "^3.7",
    "http-interop/http-factory-guzzle": "^1.2",
    "illuminate/redis": "^9.16",
    "intervention/image": "^2.7",
    "jikan-me/jikan": "^3.0",
    "laravel/lumen-framework": "^9.0",
    "laravel/tinker": "^2.7",
    "league/flysystem": "^3.0",
    "monicahq/laravel-cloudflare": "^3.2",
    "rennokki/laravel-eloquent-query-cache": "^3.3.0",
    "rogervila/lumen-rate-limiting": "^1.1",
    "staudenmeir/eloquent-has-many-deep": "^1.15",
    "suin/php-rss-writer": "^1.6"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "classmap": [
      "app/Classes"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ],
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate --ansi"
    ]
  }
}
